#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int main() {

	freopen("helpcross.in", "r", stdin);
	freopen("helpcross.out", "w", stdout);

	multiset<int> chickens;
	int c, n;
	cin >> c >> n;

	for(int i = 0; i < c; i++) {

		int insert;
		cin >> insert;
		chickens.insert(insert);

	}

	vector<pair<int, int>> cows(n);
	for(int i = 0; i < n; i++) {

		cin >> cows[i].second >> cows[i].first;

	}

	sort(cows.begin(), cows.end());

	int ans = 0;

	for(int i = 0; i < n; i++) {

		auto check = chickens.lower_bound(cows[i].second);
		if(check != chickens.end() && *check <= cows[i].first) {

			ans++;
			chickens.erase(check);

		}

	}

	cout << ans << endl;

}
