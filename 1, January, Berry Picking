#include <bits/stdc++.h>
using namespace std;
typedef long long ll;

int mod;

bool srot(int a, int b) {

	return a > b;

}

bool cmp(int a, int b) {

	return (a % mod) > (b % mod);

}

int main() {

	freopen("berries.in", "r", stdin);
	freopen("berries.out", "w", stdout);

	int n, k;
	cin >> n >> k;

	vector<int> trees(n);
	for(int i = 0; i < n; i++) {

		cin >> trees[i];

	}

	sort(trees.begin(), trees.end(), srot);
	int biggest = trees[0];
	int ans = 0;
	for(int b = 1; b <= biggest; b++) {

		int cur = 0;
		int count = 0;
		for(int i = 0; i < n; i++) {

			count += trees[i] / b;

		}

		if(count >= k) {

			ans = max(ans, b * (k / 2));
			continue;

		} else if(count < k / 2) {

			break;

		}

		mod = b;
		sort(trees.begin(), trees.end(), cmp);
		cur = b * (count - k / 2);
		for(int i = 0; i < n && (i + count) < k; i++) {

			cur += trees[i] % b;

		}

		ans = max(ans, cur);

	}

	cout << ans << endl;

}
